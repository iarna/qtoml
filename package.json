  },
  "license": "ISC",
  "scripts": {
    "prerelease": "npm t",
    "release": "standard-version -s",
    "postrelease": "npm publish && git push --follow-tags",
    "pretest": "standard",
    "test": "tap -J --100 test/*.js",
    "update-coc": "weallbehave -o . && git add CODE_OF_CONDUCT.md && git commit -m 'docs(coc): updated CODE_OF_CONDUCT.md'",
    "update-contrib": "weallcontribute -o . && git add CONTRIBUTING.md && git commit -m 'docs(contributing): updated CONTRIBUTING.md'"
  },
  "devDependencies": {
    "weallbehave": "*",
    "weallcontribute": "*",
    "standard": "*",
    "standard-version": "*",
    "tap": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iarna/qtoml.git"
  },
  "bugs": {
    "url": "https://github.com/iarna/qtoml/issues"
  },
  "homepage": "https://npmjs.com/package/qtoml",
  "main": "qtoml.js",
  "dependencies": {
    "@iarna/toml": "^2.2.1"
  },
  "description": "Use TOML to construct object literals directly in JavaScript",
  "version": "1.0.0",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "toml",
    "template-string"
  ],
  "files": [
    "qtoml.js"
  ]
}
